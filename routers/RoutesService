package routers

import (
	"github.com/gin-gonic/gin"
	"net/http"
)

func RegisterRoutes(router *gin.Engine) {
	router.GET("/", func(c *gin.Context) {
		c.JSON(http.StatusOK, gin.H{"message": "RRRRRRRRRR"})
	})

	services.CreateCustomer()
	router.GET("/Customer/Create", func(c *gin.Context) {
		c.JSON(http.StatusOK, gin.H{"message": "RRRRRRRRRR"})
	})

	// 在這裡添加更多路由
}

